[
  <% j = 0 %>
  <% @users.each do |user| %>
  {
    "id": <%= user.id %>,
    "email": "<%= user.email %>",
    "username": "<%= user.username %>",
    "info": "<%= user.info %>",
    "skills": [
    <% i = user.skills.length %>
    <% i = 1 %>
    <% user.skills.each do |skill| %>
      "<%= skill.skill %>"<% if i < user.skills.length %>,<% end %>
      <% i += 1 %>
    <% end %>
    <% j += 1 %>
    ]
  }<% if j < @users.length %>,<% end %>
  <% end %>
]

